<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#ff000000" />
            <corners android:radius="@dimen/lay_button_corner_round"/>
        </shape>
    </item>
    <item android:left="@dimen/lay_button_border_left" android:right="@dimen/lay_button_border_right"
        android:top="@dimen/lay_button_border_top" android:bottom="@dimen/lay_button_border_bottom">
        <shape android:shape="rectangle">
            <solid android:color="@color/white" />
            <corners android:radius="@dimen/lay_button_corner_round"/>
        </shape>
    </item>
</layer-list>